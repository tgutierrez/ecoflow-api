# EcoFlow API Production Environment Variables
# Copy this file to .env and fill in your actual values

# EcoFlow API Credentials (Required)
ECOFLOW_ACCESS_KEY=your_ecoflow_access_key_here
ECOFLOW_SECRET_KEY=your_ecoflow_secret_key_here

# API Configuration
ECOFLOW_BASE_URL=https://api-a.ecoflow.com/iot-open

# Server Configuration
NODE_ENV=production
PORT=3000

# Optional: Logging
LOG_LEVEL=info
